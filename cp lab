#include<stdio.h>
int main()
{ int a;
float b;
char c;

printf("enter any integer");
scanf("%d", &a);
printf("enter any floating value");
scanf("%f", &b);
printf("enter any character");
scanf(" %c", &c);

printf("integer is %d \n", a);
printf("floating value is %0.2f \n", b);
printf("character is %c \n", c);
return 0;
}
