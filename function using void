#include <stdio.h>
void namaste(); 
void bonjour();
void error();
int main()
{
    char ch, f, i;
    printf("enter your nationality \n f for french: \n i for indian: \n");
    scanf("%c", &ch);
    if (ch == 'f')
    {
        bonjour();
    }
    else if (ch == 'i')
    {
        namaste();
    }
    else
    {
        error();
    }
    return 0;
}
void namaste()
{
    printf("namaste");
}
void bonjour()
{
    printf("bonjour");
}
void error()
{
    printf("wrong character entered");
}
