#include <stdio.h>
int main()
{
    // break or continue can be used for these program as per use 
    int i, age;
    for (i = 0; i < 13; i+1)
    {
        printf("%d \n enter your age: \n", i++);
        scanf("%d", &age);
        if (age < 18)
        {
            continue;
        }
        printf("dirajveer");
    }
    return 0;
}
